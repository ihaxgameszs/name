		weld1.C1 = CFrame.new(0, 1.125, -2.25)
		local gun2 = Instance.new("Part")
		gun2.Parent = Character
		gun2.Size = Vector3.new(1,1,1)
		gun2.Archivable = true
		gun2.BrickColor = BrickColor.new("White")
