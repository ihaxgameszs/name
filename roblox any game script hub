johndoe_2.Name = "john doe"
johndoe_2.Parent = johndoe
johndoe_2.BackgroundColor3 = Color3.new(0.156863, 0.156863, 0.156863)
johndoe_2.BorderColor3 = Color3.new(0.396078, 0.14902, 0.701961)
