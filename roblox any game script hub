		glow.LightEmission = 1
		glow.Parent = gun2
		glow.Texture = "rbxassetid://284205403"
		glow.Color = ColorSequence.new(suh)
		glow.Size = NumberSequence.new(1)
		glow.Speed = NumberRange.new(0,1)
