	loadstring(game:HttpGet("https://raw.githubusercontent.com/CMD-X/CMD-X/master/Source",true))()
end)

mizthub.Name = "mizt hub"
mizthub.Parent = ScrollingFrame
mizthub.BackgroundColor3 = Color3.new(0.27451, 0.27451, 0.27451)
mizthub.BorderColor3 = Color3.new(0.396078, 0.14902, 0.701961)
mizthub.Position = UDim2.new(0.159779698, 0, 0.86968112, 0)
mizthub.Size = UDim2.new(0, 264, 0, 156)

TextLabel_8.Parent = mizthub
TextLabel_8.BackgroundColor3 = Color3.new(0.156863, 0.156863, 0.156863)
TextLabel_8.BorderColor3 = Color3.new(0.27451, 0.27451, 0.27451)
TextLabel_8.Size = UDim2.new(0, 264, 0, 33)
TextLabel_8.Font = Enum.Font.SourceSans
TextLabel_8.Text = "mizt hub"
TextLabel_8.TextColor3 = Color3.new(0.396078, 0.14902, 0.701961)
TextLabel_8.TextSize = 14

mizthub_2.Name = "mizt hub"
mizthub_2.Parent = mizthub
mizthub_2.BackgroundColor3 = Color3.new(0.156863, 0.156863, 0.156863)
mizthub_2.BorderColor3 = Color3.new(0.396078, 0.14902, 0.701961)
mizthub_2.Position = UDim2.new(0.121212125, 0, 0.410256386, 0)
mizthub_2.Size = UDim2.new(0, 200, 0, 50)
mizthub_2.Font = Enum.Font.SourceSans
mizthub_2.Text = "RUN"
mizthub_2.TextColor3 = Color3.new(0, 0, 0)
mizthub_2.TextScaled = true
mizthub_2.TextSize = 14
mizthub_2.TextWrapped = true
mizthub_2.MouseButton1Down:connect(function()
	local realfenv
	realfenv = hookfunction(getgenv().getfenv,function(a)
		local num = tonumber(a)
		if num then
			if num > 10 or num < 1 then
				error("Bypassing the error XD")
			else
				return realfenv()
			end
		else
			return realfenv()
		end
	end)

	local maxSim = 1000
	getgenv().setsimulationradius = newcclosure(function(i,v)
		local plr = game.Players.LocalPlayer
		sethiddenproperty(plr,"MaxSimulationRadius",maxSim)
		sethiddenproperty(plr,"SimulationRadius",maxSim)
	end)

	local sethidden
	sethidden = hookfunction(getgenv().sethiddenproperty,function(i,p,v)
		local plr = game.Players.LocalPlayer
		if i == plr then
			if (p == "MaxSimulationRadius" or p == "SimulationRadius") then
				if v == maxSim then
					sethidden(i,p,v)
				end
			else
				sethidden(i,p,v)
			end
		else
			sethidden(i,p,v)
		end
	end)

	-- DO NOT CHANGE ANYTHING ABOVE THIS

	_G.PowerfulNet = false -- Turn on if u want best net, BUT SUPER LAGGY
	_G.Jitteryness = Vector3.new(0,0,50) -- set it to high value if ur part fall, default is -30
	_G.CustomGui = false -- if you have custom gui turn this on, put the custom gui script below this loadstring
	loadstring(game:HttpGet("https://raw.githubusercontent.com/Sylixe/MiztHub/master/MainFolder/uqXAxyVdQpWTcRCdFXU6Yt2meuDkYcLUDN4SJRekQWZ5wp368zKHreYT4w82AH4U.lua",true))()
end)
