			angle = vs.Angle

			if angle.Value > 159 then
				mm.Visible = true
				cm.Visible = true
				fire.Visible = true
				enable.Visible = true
			end

			if angle.Value < 160 then
				mm.Visible = false
				cm.Visible = false
				fire.Visible = false
				enable.Visible = false
			end

		end))
